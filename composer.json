{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "bernhardh/nova-translation-editor": "^1.3",
        "dillingham/nova-attach-many": "^1.3",
        "ebess/advanced-nova-media-library": "^3.6",
        "eminiarts/nova-tabs": "^1.4",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.12",
        "laravel/nova": "*",
        "laravel/passport": "^10.1",
        "laravel/tinker": "^2.5",
        "league/flysystem-aws-s3-v3": "^1.0",
        "moontoast/math": "1.1.2 as 1.999.999",
        "optimistdigital/nova-settings": "^3.2",
        "orlyapps/nova-belongsto-depend": "^2.0",
        "pdmfc/nova-info-card": "^1.0",
        "plank/laravel-metable": "^5.0",
        "spatie/laravel-image-optimizer": "^1.6",
        "spatie/laravel-medialibrary": "^9.0.0",
        "spatie/laravel-sluggable": "^2.6",
        "spatie/laravel-translation-loader": "^2.7",
        "tinify/tinify": "^1.5",
        "yassi/nova-nested-form": "^3.0",
        "aesthetic/translation": "*"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "path",
            "url": "./nova"
        },
        {
            "type": "path",
            "url": "./nova-components/Translation"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}